<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <TargetFramework>net7.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>NTDLS.FastMemoryCache</Title>
    <Copyright>Copyright Â© 2023 NetworkDLS</Copyright>
    <Description>
      Provides fast and easy to use partitioned memory cache for .net that helps manage the maximum size and track performance.
    </Description>
    <PackageProjectUrl>https://github.com/NTDLS/NTDLS.FastMemoryCache</PackageProjectUrl>
    <RepositoryUrl>https://github.com/NTDLS/NTDLS.FastMemoryCache</RepositoryUrl>
    <PackageTags>memory-cache;partitioned-cache;managed-cache;fast-cache;cache;memory</PackageTags>
    <PackageReleaseNotes>
      Added configurable partitioned memory constructor.
    </PackageReleaseNotes>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Company>NetworkDLS</Company>
    <AssemblyVersion>1.1.4</AssemblyVersion>
    <FileVersion>1.1.4</FileVersion>
    <VersionPrefix>1.1.4</VersionPrefix>
    <version></version>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>64.png</PackageIcon>
    <ApplicationIcon>NTDLS.FastMemoryCache.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="NTDLS.FastMemoryCache.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\Images\64.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>

    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="NTDLS.Semaphore" Version="1.3.1" />
    <PackageReference Include="protobuf-net" Version="3.1.25" />
  </ItemGroup>
</Project>